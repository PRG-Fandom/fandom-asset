<infobox_custom-1>
    <right>
        <detail>
            <div class="top">
                <div class="horizontal_line"></div>
                <div class="title">
                    <div class="name">{{{Name|{{#explode:{{PAGENAME}}|<nowiki />-<nowiki />|1}}}}}</div>
                    <div class="frame">{{{Frame|{{#explode:{{PAGENAME}}|<nowiki />-<nowiki />|2}}}}}</div>
                </div>
            </div>
            <div class="middle">
                <div class="vertical-line"></div>
                <div class="detail">
                    <div class="service">
                        <header>Service</header>
                        <content>{{{Service Time|}}}</content>
                    </div>
                    <div class="psychological">
                        <header>Psychological Age</header>
                        <content>{{{Psychological Age|}}} Years old</content>
                    </div>
                    <div class="activation">
                        <header>Activation Date</header>
                        <content>{{{Activation}}}</content>
                    </div>
                    <div class="height">
                        <header>Height</header>
                        <content>{{{Height|}}}</content>
                    </div>
                    <div class="weight">
                        <header>Weight</header>
                        <content>{{{Weight|}}}kg</content>
                    </div>
                    <div class="vital-fluid">
                        <header>Vital Fluid Type</header>
                        <content>{{{Vital Fluid|}}}</content>
                    </div>
                </div>
                <div class="stats">
                    <div class="stat_review">
                        <header>Stat</header>
                        <underline>Review</underline>
                    </div>
                    <div class="number">
                        <div class="hp">
                            <header>HP</header>
                            <number>{{{HP|}}}</number>
                        </div>
                        <div class="atk">
                            <header>ATK</header>
                            <number>{{{ATK|}}}</number>
                        </div>
                        <div class="def">
                            <header>DEF</header>
                            <number>{{{DEF|}}}</number>
                        </div>
                        <div class="crit">
                            <header>CRIT</header>
                            <number>{{{CRIT|}}}</number>
                        </div>
                    </div>
                    <div class="graph"></div>
                </div>
            </div>
            <div class="separation_line"></div>
            <div class="bottom">
                <div class="rank">
                    <header>Rank</header>
                    <content>
                        {{#switch:{{{Rank}}}
                            |S= {{icon|Name=|Icon=1|Size=60|Unclickable=1}}
                            |A= {{icon|Name=|Icon=1|Size=60|Unclickable=1}}
                            |B= {{icon|Name=|Icon=1|Size=60|Unclickable=1}}
                            |Error
                        }}
                    </content>
                </div>
                <div class="class">
                    <header>Class</header>
                    <content>
                        {{#switch: {{{Class}}}
                            |Attacker=   {{icon|Name=|Icon=1|Size=60|Unclickable=1}}
                            |Tank=       {{icon|Name=|Icon=1|Size=60|Unclickable=1}}
                            |Support=    {{icon|Name=|Icon=1|Size=60|Unclickable=1}}
                            |Compositer= {{icon|Name=|Icon=1|Size=60|Unclickable=1}}
                            |Error
                        }}
                    </content>
                </div>
                <div class="weapon">
                    <header>Weapon</header>
                    <content>{{icon|Name=|Size=60|Category=1|Link={{{Weapon}}}}}</content>
                </div>
                <div class="exclusive-weapon">
                    <header>Exclusive Weapon</header>
                    <content>{{icon|Name=|Size=60|Link={{{Weapon}}}}}</content>
                </div>
                <div class="faction">
                    <header>Faction</header>
                    <content>{{icon|Name=|Size=60|Link={{{Faction}}}}}</content>
                </div>
                <div class="gift">
                    <header>Favorite Gift</header>
                    <content>{{icon|Name=|Size=60|Unclickable=1}}</content>
                </div>
            </div>
        </detail>
    </right>
</infobox_custom-1>